{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sr-adf-dev"
		},
		"azuredb_source_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azuredb_source'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=srsource.database.windows.net;Initial Catalog=srsource;User ID=sr_admin"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakesource7046.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "cdd161d3-7ba3-47f0-a0f2-086ab04ddc0e"
		},
		"AzureDataLakeStorage1_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "bab8a2c6-5680-4e9c-b5cd-7e6f57491fbc"
		},
		"AzureDataLakeStorage_sink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://srdevsink.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage_sink_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "cdd161d3-7ba3-47f0-a0f2-086ab04ddc0e"
		},
		"AzureDataLakeStorage_sink_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "bab8a2c6-5680-4e9c-b5cd-7e6f57491fbc"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://srkeyvoultdev.vault.azure.net/"
		},
		"RestService_source_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://airlabs.co/api/v9/airlines"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sr-adf-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"azuredb_source_connectionString": {
						"value": "[parameters('azuredb_source_connectionString')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"RestService_source_properties_typeProperties_url": {
						"value": "[parameters('RestService_source_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureDataLakeStorage_sink_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_sink_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage_sink_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStorage_sink_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStorage_sink_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureDataLakeStorage_sink_properties_typeProperties_servicePrincipalId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}